# Talker-Thinker Environment Configuration
# Copy this file to .env and fill in your values

# LLM API Keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Model Configuration
TALKER_MODEL=gpt-4o-mini
THINKER_MODEL=gpt-4o

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/talker_thinker

# Vector Database Configuration
VECTOR_DB_URL=http://localhost:6333

# Monitoring
LOG_LEVEL=INFO
ALERT_WEBHOOK_URL=

# Feature Flags
ENABLE_STREAMING=true
ENABLE_PROGRESS_NOTIFICATION=true
ENABLE_PROACTIVE_CONVERSATION=true
